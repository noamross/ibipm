  Keywords                 = {Agelaius phoeniceus, Anthropogenic Habitats, Brood Parasitism, Human-Commensal Predators, Red-Winged Blackbirds, Source-Sink Habitats},
  Publisher                = {Ecological Society of America},
  Url                      = {http://links.jstor.org/sici?sici=1051-0761%28200008%2910%3A4%3C1211%3ASASHOR%3E2.0.CO%3B2-X}
