  Jstor_formatteddate      = {Nov., 1996},
  Keywords                 = {egg limitation, host-feeding, parasitoid oviposition behaviour, population dynamics, stability},
  Publisher                = {British Ecological Society},
  Url                      = {http://links.jstor.org/sici?sici=0021-8790%28199611%2965%3A6%3C743%3ATEOELO%3E2.0.CO%3B2-X}
